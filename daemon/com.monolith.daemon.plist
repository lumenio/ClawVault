<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.monolith.daemon</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/MonolithDaemon</string>
    </array>
    <key>MachServices</key>
    <dict>
        <key>com.monolith.daemon</key>
        <true/>
    </dict>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/monolith-daemon.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/monolith-daemon.err</string>
</dict>
</plist>
